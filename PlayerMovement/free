extends KinematicBody2D

var movingup
var velocity = Vector2()
var moveSpeed = 500

func _ready():
	movingup = true

func get_input():
	if Input.is_action_pressed("moveDOWN"):
		movingup = true
		velocity.y = moveSpeed
	elif Input.is_action_pressed("moveUP"):
		movingup = false
		velocity.y = -moveSpeed
	else:
		velocity.y = 0

func _physics_process(delta):
	get_input()
	move_and_slide(velocity)
